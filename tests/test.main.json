{
    "Config": {
        "Main": "EVREST",
        "Templates": [
            "templates/rest.tmpl",
            "templates/handler.tmpl",
            "templates/rest.handler.tmpl",
            "templates/rest.help.tmpl",
            "templates/rest.prometheus.tmpl",
            "templates/rest.connect.bolt.tmpl",
            "templates/rest.connect.log.tmpl",
            "templates/rest.connect.user.tmpl",
            "templates/rest.connect.session.tmpl",
            "templates/rest.connect.secret.tmpl",
            "templates/rest.connect.token.tmpl",
            "templates/service.evauth.tmpl",
            "templates/service.evbolt.tmpl",
            "templates/service.evlog.tmpl"
        ],
        "Imports": [
            "fmt",
            "flag",
            "os",
            "strings",
            "log",
            "net/http",
            "github.com/prometheus/client_golang/prometheus",
            "github.com/prometheus/client_golang/prometheus/promhttp",
            "github.com/dchest/uniuri",
            "github.com/gorilla/mux",
            "evalgo.org/eve",
            "net/url",
            "encoding/base64",
            "errors"
        ],
        "Vars": {
            "COOKIE_EXP_MINUTES": 15,
            "DEBUG": true,
            "DefaultAddress": "0.0.0.0:9090",
            "Description": "EVBolt is a rest micro service which wrapps the golang bolt database",
            "ENABLE_CROSS_ORIGIN": true,
            "EVLOG_URL": "",
            "Name": "EVBolt",
            "Package": "main",
            "ROUTE_PATH_PREFIX": "/0.0.1/eve/",
            "SECRET_ENC_KEY": "TokenKeyEnc",
            "SECRET_KEY_FOR_TOKEN": "123456789012345678901234567890ab",
            "SECRET_SIG_FOR_TOKEN": "sig.key.secret",
            "SECRET_SIG_KEY": "TokenKeySig",
            "SECRET_STORAGE_BUCKET": "secrets",
            "SECRET_STORAGE_DB": "secrets.db",
            "SECRET_STORAGE_URL": "http://localhost:9090/0.0.1/eve/bolt",
            "SESSION_STORAGE_BUCKET": "sessions",
            "SESSION_STORAGE_DB": "sessions.db",
            "SESSION_STORAGE_URL": "http://localhost:9090/0.0.1/eve/bolt",
            "Src": "https://git.evalgo.de:8443/",
            "TOKEN_EXP_DAYS": 7,
            "TOKEN_STORAGE_BUCKET": "tokens",
            "TOKEN_STORAGE_DB": "tokens.db",
            "TOKEN_STORAGE_URL": "http://localhost:9090/0.0.1/eve/bolt",
            "URLS": [
                "/help",
                "/bolt",
                "/metrics"
            ],
            "USER_STORAGE_BUCKET": "users",
            "USER_STORAGE_DB": "users.db",
            "USER_STORAGE_URL": "http://localhost:9090/0.0.1/eve/bolt",
            "USE_EVBOLT_API": true,
            "USE_EVBOLT_AUTH": true,
            "USE_EVLOG": false,
            "USE_EVLOG_API": false,
            "USE_EVSECRET": true,
            "USE_EVSESSION": true,
            "USE_EVTOKEN": true,
            "USE_EVUSER": true,
            "USE_LOGIN_API": false,
            "USE_PROMETHEUS": true,
            "UsageFunc": "EVUsage",
            "Version": "0.0.1"
        },
        "Commands": [{
            "Name": "help",
            "Desc": "help command to be used for detailed information",
            "Flags": [{
                    "FName": "hhttp",
                    "FType": "string",
                    "FValue": "",
                    "FDesc": "display the help menu as a html website for the given command"
                },
                {
                    "FName": "debug",
                    "FType": "bool",
                    "FValue": "false",
                    "FDesc": "display debug information for the given command"
                },
                {
                    "FName": "version",
                    "FType": "string",
                    "FValue": "0.0.1",
                    "FDesc": "version of the running command"
                },
                {
                    "FName": "address",
                    "FType": "string",
                    "FValue": "127.0.0.1:9090",
                    "FDesc": "address for the http service to run on the given command"
                },
                {
                    "FName": "crt",
                    "FType": "string",
                    "FValue": "",
                    "FDesc": "path to the ssl certificate"
                },
                {
                    "FName": "key",
                    "FType": "string",
                    "FValue": "",
                    "FDesc": "path to the ssl private key"
                }
            ]
        }]
    }
}